{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "[javascript]":  {
    "editor.formatOnSave": true,
  },
  "[javascriptreact]":  {
    "editor.formatOnSave": true,
  },
  "[typescript]":  {
    "editor.formatOnSave": true,
  },
  "[typescriptreact]":  {
    "editor.formatOnSave": true,
  },
  "prettier.singleQuote": true,
  "prettier.eslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "prettier.printWidth": 70,
  "prettier.trailingComma": "all"
}
